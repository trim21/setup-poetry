{
  "name": "setup-python-poetry",
  "version": "2.0.5",
  "private": true,
  "description": "install poetry in on your OS",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint ./",
    "format": "prettier -w .",
    "build": "npx esbuild --bundle --platform=node --packages=bundle --legal-comments=external --sourcemap=inline --target=node20 ./src/main.ts --outfile=dist/index.js",
    "test": "jest",
    "prepare": "husky install"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.6.5",
  "engines": {
    "node": "^20.0.0"
  },
  "dependencies": {
    "@actions/cache": "4.0.3",
    "@actions/core": "1.11.1",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.2.3",
    "@renovatebot/pep440": "4.1.0",
    "source-map-support": "0.5.21"
  },
  "lint-staged": {
    "*.{mjs,js,ts,yaml,md,yml,json}": "prettier -w"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.11",
    "@types/source-map-support": "0.5.10",
    "esbuild": "0.25.1",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "3.9.1",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.4.0",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "ts-jest": "29.2.6",
    "typescript": "5.8.2",
    "typescript-eslint": "8.27.0"
  }
}
