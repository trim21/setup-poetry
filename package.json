{
  "name": "setup-python-poetry",
  "version": "1.1.11",
  "private": true,
  "description": "install poetry in on your OS",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ext ts,js ./",
    "format": "eslint --ext ts,js --fix ./",
    "build": "npx esbuild --bundle --platform=node --legal-comments=external --sourcemap=inline --target=node16 ./src/main.ts --outfile=dist/index.js",
    "test": "jest",
    "all": "npm run lint && npm run build && npm test",
    "postversion": "git push"
  },
  "license": "MIT",
  "packageManager": "yarn@3.6.1",
  "engines": {
    "node": ">=16.16.0"
  },
  "dependencies": {
    "@actions/cache": "3.2.1",
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.1.0",
    "@renovatebot/pep440": "2.1.18"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "esbuild": "0.18.17",
    "eslint": "8.46.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.6.2",
    "jest-circus": "29.6.2",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  }
}
