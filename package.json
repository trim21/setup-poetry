{
  "name": "setup-python-poetry",
  "version": "1.1.12",
  "private": true,
  "description": "install poetry in on your OS",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ext ts,js ./",
    "format": "eslint --ext ts,js --fix ./",
    "build": "npx esbuild --bundle --platform=node --legal-comments=external --sourcemap=inline --target=node16 ./src/main.ts --outfile=dist/index.js",
    "test": "jest",
    "all": "npm run lint && npm run build && npm test",
    "postversion": "git push"
  },
  "license": "MIT",
  "packageManager": "yarn@3.6.3",
  "engines": {
    "node": ">=16.16.0"
  },
  "dependencies": {
    "@actions/cache": "3.2.2",
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.1.1",
    "@renovatebot/pep440": "2.1.20",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@types/jest": "29.5.4",
    "@types/node": "18.17.12",
    "@types/source-map-support": "0.5.7",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "esbuild": "0.19.2",
    "eslint": "8.48.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.6.4",
    "jest-circus": "29.6.4",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
