{
  "name": "setup-python-poetry",
  "version": "2.0.5",
  "private": true,
  "description": "install poetry in on your OS",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint ./",
    "format": "prettier -w .",
    "build": "npx esbuild --bundle --platform=node --packages=bundle --legal-comments=external --sourcemap=inline --target=node20 ./src/main.ts --outfile=dist/index.js",
    "test": "jest",
    "prepare": "husky install"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": "^20.0.0"
  },
  "dependencies": {
    "@actions/cache": "3.2.4",
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.2.3",
    "@renovatebot/pep440": "3.0.20",
    "source-map-support": "0.5.21"
  },
  "lint-staged": {
    "*.{mjs,js,ts,yaml,md,yml,json}": "prettier -w"
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.13",
    "@types/node": "20.16.10",
    "@types/source-map-support": "0.5.10",
    "esbuild": "0.24.0",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.3.0",
    "eslint-plugin-unicorn": "55.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "typescript": "5.5.4",
    "typescript-eslint": "8.8.0"
  }
}
