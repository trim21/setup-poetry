{
  "name": "setup-python-poetry",
  "version": "2.0.3",
  "private": true,
  "description": "install poetry in on your OS",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ext ts,js ./",
    "format": "prettier -w .",
    "build": "npx esbuild --bundle --platform=node --legal-comments=external --sourcemap=inline --target=node16 ./src/main.ts --outfile=dist/index.js",
    "test": "jest",
    "all": "npm run lint && npm run build && npm test",
    "postinstall": "husky install",
    "postversion": "git push"
  },
  "license": "MIT",
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": "^20.0.0"
  },
  "dependencies": {
    "@actions/cache": "3.2.2",
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.2.0",
    "@renovatebot/pep440": "3.0.17",
    "source-map-support": "0.5.21"
  },
  "lint-staged": {
    "*.{js,ts,yaml,md,yml,json}": "prettier -w"
  },
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@types/node": "20.10.6",
    "@types/source-map-support": "0.5.10",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "esbuild": "0.19.8",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "typescript": "5.3.2"
  }
}
